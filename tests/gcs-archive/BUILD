package(default_visibility = ["//visibility:public"])

load("@k8s_object//:defaults.bzl", "k8s_object")

k8s_object(
    name = "template",
    template = "template.yaml",
)

k8s_object(
    name = "build",
    template = "build.yaml",
)

load("@io_bazel_rules_k8s//k8s:objects.bzl", "k8s_objects")

k8s_objects(
    name = "test",
    objects = [
        ":template",
        ":build",
    ],
)
